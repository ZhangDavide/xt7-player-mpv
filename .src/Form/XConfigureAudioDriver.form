          Border = False
          { JackParseMeHBox8 HBox
            MoveScaled(0,0,45,6)
            AutoResize = True
            Padding = 8
            { CheckBox18jack CheckBox
              MoveScaled(1,2,17,4)
              #Translate = False
              Tag = "port="
              Text = "Port"
            }
            { TextBox3jack TextBox
              MoveScaled(19,1,22,4)
              #Translate = False
              Expand = True
            }
          }
          { JackParseMeHBox1 HBox
            MoveScaled(0,6,45,6)
            AutoResize = True
            Padding = 8
            { CheckBox1jack CheckBox
              MoveScaled(1,2,17,4)
              #Translate = False
              Tag = "name="
              Text = "Name"
            }
            { TextBox4jack TextBox
              MoveScaled(19,1,22,4)
              #Translate = False
              Expand = True
              Text = "mpv"
            }
          }
        }
        Index = 2
        Text = ("Pulse")
        { pulsescrollview ScrollView
          MoveScaled(1,2,56,30)
          Expand = True
          Arrangement = Arrange.Vertical
          Border = False
          { PulseParseMeHbox1 VBox
            MoveScaled(1,0,45,13)
            Padding = 6
            { PulseDriverChk2pulse CheckBox
              MoveScaled(1,1,29,4)
              #Translate = False
              Text = "host:output sink"
            }
            { TextBox6Alsapulse TextBox
              MoveScaled(1,6,41,4)
            }
          }
          { PulseParseMeHbox3 HBox
            MoveScaled(0,15,46,6)
            AutoResize = True
            Padding = 6
            { AlsaDeviceChk3pulse CheckBox
              MoveScaled(1,1,18,4)
              #Translate = False
              Tag = "buffer="
              Text = "buffer"
            }
            { ComboBox1pulse ComboBox
              MoveScaled(21,1,24,4)
              #Translate = False
              List = ["native", "100", "250", "500", "750", "1000", "1250", "1500", "1750", "2000"]
              Text = "250"
            }
          }
          { PulseParseMeHbox2 HBox
            MoveScaled(0,22,46,6)
            AutoResize = True
            Padding = 6
            { AlsaDeviceChk4pulse CheckBox
              MoveScaled(1,1,18,4)
              #Translate = False
              Tag = "latency-hacks="
              Text = "latency-hacks"
            }
            { ComboBox2pulse ComboBox
              MoveScaled(21,1,24,4)
              #Translate = False
              ReadOnly = True
              List = ["yes", "no"]
              Text = "no"
            }
          }
        }
        Index = 3
        Text = ("Pcm")
        { pcmscrollview ScrollView
          MoveScaled(2,2,54,29)
          Expand = True
          Arrangement = Arrange.Vertical
          Border = False
          { PcmParseMeHBox3 HBox
            MoveScaled(0,0,45,6)
