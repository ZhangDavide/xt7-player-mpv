              Text = "mixer-name"
            }
            { AlsaDeviceTextBox2 ComboBox
              MoveScaled(18,1,26,4)
              #Translate = False
              Font = Font["Monospace"]
              Expand = True
              List = ["Master", "PCM", "Headphone", "Front", "Center", "Surround", "LFE"]
              Text = "Master"
            }
          }
          { AlsaParseMeHbox2 HBox
            MoveScaled(0,6,46,6)
            AutoResize = True
            Padding = 6
            { AlsaDeviceChk1 CheckBox
              MoveScaled(1,1,15,4)
              #Translate = False
              Tag = "mixer-index="
